<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.cardtry">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity" android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".Level"/>
        <activity android:name=".AnimalsLevelMedium"/>
        <activity android:name=".AnimalsLevelEasy"/>
        <activity android:name=".AnimalsLevelHard"/>
        <activity android:name=".SeaLevelEasy"/>
        <activity android:name=".SeaLevelMedium"/>
        <activity android:name=".SeaLevelHard"/>
        <activity android:name=".SpaceLevelEasy"/>
        <activity android:name=".SpaceLevelMedium"/>
        <activity android:name=".SpaceLevelHard"/>
        <activity android:name=".WorldActivity" android:launchMode="singleTask"/>
    </application>

</manifest>